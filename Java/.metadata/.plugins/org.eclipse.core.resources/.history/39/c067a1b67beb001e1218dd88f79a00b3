package poligoni;

import luoghi.*;

public class Quadrato extends Rettangolo {
	Segmento lato;
	
	public Quadrato(Segmento lato) {
		super(lato, lato);
		this.nome = "Quadrato";
	}

	@Override
	public String toString() {
		return "Quadrato [lato=" + lato + ", perimetro()=" + perimetro() + ", area()=" + area() + "]";
	}
	
	
}
